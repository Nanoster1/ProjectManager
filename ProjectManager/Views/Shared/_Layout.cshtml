
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - ProjectManager</title>
    <link rel="stylesheet" href="~/ProjectManager.styles.css" asp-append-version="true"/>
    <link rel="stylesheet" href="/css/site.css" type="text/css"/>
</head>
<body>
<div class="column-wrapper">
<header>
    <div class="head">
        <div class="ico pad"><a href="#"><img src="~/assets/images/logo.png" width="100" height="50"></a></div>
        <div class="profile pad">
            <div id="sign-in" class="ml-mr-20 sing">
                <a href="#" class="img-n-text">
                    <span class="header-text default hover-text">Sign in</span>
                </a>
            </div>
            <div id="sign-up" class="sing tracing">
                <a href="#" class="img-n-text">
                    <span class="header-text default hover-text">Sign up</span>
                </a>
            </div>
        </div>
    </div>
</header>
<div class="flex-1-0">
    <main>
        @RenderBody()
    </main>
</div>
<footer class="footer footer-tittle-text flex-0-0">
    <div class="container2">
        <div class="container-horizontal">
            <section class="footer-section">
                <p class="title">
                    More to Explore
                </p>
                <ul class="footer-list footer-text">
                    <li>
                        <a class="hover-text" href="#">About Us</a>
                    </li>
                    <li>
                        <a class="hover-text" href="#">Privacy Policy</a>
                    </li>
                </ul>
            </section>
            <section class="footer-section">
                <p class="title">
                    Our Social
                </p>
                <ul class="footer-list footer-text">
                    <li>
                        <a href="https://github.com/Nanoster1/ProjectManager" class="img-n-text">
                            <img width="16" height="16" role="img" src="~/assets/images/github.png" class="footer-icon shadow">
                            <span class="hover-text">GitHub</span>
                        </a>
                    </li>
                    <li>
                        <a href="https://github.com/Nanoster1/ProjectManager" class="img-n-text">
                            <img width="16" height="16" role="img" src="~/assets/images/discord.svg" class="footer-icon shadow">
                            <span class="hover-text">Discord</span>
                        </a>
                    </li>
                    <li>
                        <a href="https://github.com/Nanoster1/ProjectManager" class="img-n-text">
                            <img width="16" height="16" role="img" src="~/assets/images/telegram.png" class="footer-icon shadow">
                            <span class="hover-text">Telegram</span>
                        </a>
                    </li>
                </ul>
            </section>
        </div>
    </div>
</footer>
</div>
<script src="/js/layout..js" type="text/javascript"></script>
<script src="~/js/site.js" type="text/javascript"></script>
<script src="~/lib/jquery/dist/jquery.min.js" type="text/javascript"></script>
</body>
</html>